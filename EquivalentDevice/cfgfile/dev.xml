<?xml version="1.0" ?>
<configure>

	 <dev id="cmdRcv" class="ZmqRcvAdapter">
        
           <remoteIp>*</remoteIp>
           <!-- 接收端口号 -->
       	<remotePort>2202</remotePort>
     </dev>
	  <dev id="cmdSend" class="ZmqSendAdapter">
        
           <remoteIp>127.0.0.1</remoteIp>
           <!-- 发送端口号 -->
       	<remotePort>2203</remotePort>
     </dev>
	 <dev id="cmdParamRev" class="ZmqRcvAdapter">
           <remoteIp>*</remoteIp>
           <!-- 发送端口号 -->
       	<remotePort>2204</remotePort>
     </dev>
</configure>