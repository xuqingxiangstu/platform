<?xml version="1.0"?>	
<configure>
	<!--
	继电器控制适配器
	id：全局唯一，需按实际更改
	class：固定不变
	-->
	<dev id="PXI2569" class="RelayCtrlAdapter">			
		<!-- 设备名称，Ni-Max中查看 -->
		<rname>PXI2569</rname>
		<!-- 通道数，NI-Max查看-->
		<channelSize>100</channelSize>	
	</dev>
	
	<!--
	模拟量输入适配器
	id：全局唯一，需按实际更改
	class：固定不变
	-->
	<dev id="ssss" class="test">			
		<!-- 设备名称，Ni-Max中查看 -->
		<ip>12</ip>
	</dev>
</configure>